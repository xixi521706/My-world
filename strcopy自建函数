#define _CRT_SECURE_NO_WARNINGS 1
#include <stdio.h>
#include <assert.h>
char* My_strcopy(char* des, const char* src)
{ 
	char* ret = des;
	assert(des != NULL);
	assert(src != NULL);
	while (*des++ = *src++)
	{
		;
	}
	return ret;
}
